{
  "@attribute": {
    "name": "test_currency",
    "group_by": "patient_id",
    "column_types": {
      "amount": {
        "type": "currency"
      }
    },
    "@object": {
      "items": {
        "@array": {
          "collect": true,
          "@item": {
            "item": "{item}",
            "amount": "{amount}"
          }
        }
      }
    }
  }
}
